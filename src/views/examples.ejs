<!doctype html>
<html lang="en">

  <% include head %>

  <div class="examples">
    <h1><%= heading %></h1>
    <ul>
      <% examples.forEach(function(example) { %>
        <li>
          <a href="<%= example.url %>">
            <%= example.name %>
          </a>
          (<%= example.text %>)
        </li>
      <% }); %>
    </ul>
  </div>

</html>
